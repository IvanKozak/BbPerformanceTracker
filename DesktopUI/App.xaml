<views:MvxApplication x:Class="DesktopUI.App"
             xmlns:views="clr-namespace:MvvmCross.Platforms.Wpf.Views;assembly=MvvmCross.Platforms.Wpf"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:DesktopUI"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <Color x:Key="Primary">#03DAC6</Color>
        <Color x:Key="PrimaryVariant">#018786</Color>

        <SolidColorBrush x:Key="PrimaryBrush" Color="{StaticResource Primary}"/>
        <SolidColorBrush x:Key="PrimaryVariantBrush" Color="{StaticResource PrimaryVariant}"/>
    </Application.Resources>
</views:MvxApplication>
